{
  "name": "Griable",
  "version": "0.0.1",
  "description": "griable",
  "main": "bundle.js",
  "scripts": {
    "build:js": "webpack",
    "build:css": "sass src/styles/main.scss:dist/bundle.css",
    "build": "npm run build:js && npm run build:css",
    "watch:css": "npm run build:css -- --watch",
    "watch:js": "npm run build:js -- --watch",
    "watch": "parallelshell 'npm run watch:js' 'npm run watch:css' && 'npm run serve'",
    "serve": "browser-sync start --server"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:griable/griable.git"
  },
  "keywords": [
    "griable",
    "javascript",
    "developer"
  ],
  "author": "Gabriel Slama",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/griable/griable/issues"
  },
  "homepage": "https://github.com/griable/griable",
  "devDependencies": {
    "browser-sync": "^1.8.1",
    "parallelshell": "^1.0.3",
    "webpack": "^1.4.13"
  }
}
